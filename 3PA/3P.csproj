  <PropertyGroup>
    <PostBuildEvent>echo [INFO] End of build for 3P

set NPPFOLDERNAME=3P_notepad++
set CPUNAME=x86
set RELEASENAME=3P.zip
call :copytonotepad
call :ziprelease

set NPPFOLDERNAME=3P_notepad++x64
set CPUNAME=x64
set RELEASENAME=3P_x64.zip
call :copytonotepad
call :ziprelease

exit 0


REM zip release
:ziprelease

echo [INFO] Zipping release Releases\%25RELEASENAME%25

WHERE 7z &gt;nul 2&gt;nul
IF not "%25ERRORLEVEL%25" == "0" (
	echo 7z.exe not found in the path, can't create the .zip releases
	goto :label_end
)

md "$(SolutionDir)Releases" &gt;nul 2&gt;nul
del /Q "$(SolutionDir)Releases\%25RELEASENAME%25" &gt;nul 2&gt;nul


echo [INFO] Adding netFrameworkChecker\NetFrameworkChecker.exe

7z a "$(SolutionDir)Releases\%25RELEASENAME%25" "$(SolutionDir)netFrameworkChecker\NetFrameworkChecker.exe" &gt;nul

echo [INFO] Adding bin\$(ConfigurationName)\%25CPUNAME%25\$(TargetFileName)

7z a "$(SolutionDir)Releases\%25RELEASENAME%25" "$(ProjectDir)bin\$(ConfigurationName)\%25CPUNAME%25\$(TargetFileName)" &gt;nul
if exist "$(ProjectDir)bin\$(ConfigurationName)\%25CPUNAME%25\$(TargetName).pdb" (
    if "$(ConfigurationName)" == "Debug" (
	    7z a "$(SolutionDir)Releases\%25RELEASENAME%25" "$(ProjectDir)bin\$(ConfigurationName)\%25CPUNAME%25\$(TargetName).pdb" &gt;nul
    )
)
exit /b 0


REM copy files to the notepad++ folder for dev purposes
:copytonotepad

echo [INFO] Copying output files to ..\%25NPPFOLDERNAME%25\plugins\

if exist "$(SolutionDir)..\%25NPPFOLDERNAME%25\plugins\" ( 
	copy /Y "$(ProjectDir)bin\$(ConfigurationName)\%25CPUNAME%25\$(TargetFileName)" "$(SolutionDir)..\%25NPPFOLDERNAME%25\plugins\$(TargetName)\$(TargetFileName)" &gt;nul
	if exist "$(ProjectDir)bin\$(ConfigurationName)\%25CPUNAME%25\$(TargetName).pdb" (
		copy /Y "$(ProjectDir)bin\$(ConfigurationName)\%25CPUNAME%25\$(TargetName).pdb" "$(SolutionDir)..\%25NPPFOLDERNAME%25\plugins\$(TargetName)\$(TargetName).pdb") &gt;nul
	)
)

exit /b 0
</PostBuildEvent>
